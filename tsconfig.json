{
	"compilerOptions": {
		"allowUnreachableCode": false,
		"allowUnusedLabels": false,
		"charset": "utf-8",
		"forceConsistentCasingInFileNames": true,
		"module": "commonjs",
		"newLine": "LF",
		"noImplicitAny": true,
		"strictNullChecks": true,
		"sourceMap": false,
		"outDir": "lib",
		"noEmit": false,
		"moduleResolution": "node",
		"target": "es5",
		"experimentalDecorators": true,
		"baseUrl": ".",
		"lib": ["es5", "es2015"]
	},
	"include": ["src/**/*.ts"]
}
